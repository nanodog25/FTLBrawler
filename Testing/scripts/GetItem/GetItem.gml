///@arg itemText

switch(argument0)
{
	case global.ItemSpeedAura:
		return _items._itemSpeedAura;
	case global.ItemShield:
		return _items._itemShield;
	case global.ItemSlowShot:
		return _items._itemSlowShot;
	case global.ItemLaneBarrier:
		return _items._itemLaneBarrier;
	case global.ItemLaunchShot:
		return _items._itemLaunchShot;
	case global.ItemStunTrap:
		return _items._itemStunTrap;
	case global.ItemDownConveyor:
		return _items._itemDownConveyor;
	case global.ItemUpConveyor:
		return _items._itemUpConveyor;
}